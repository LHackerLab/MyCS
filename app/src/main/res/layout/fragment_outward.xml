<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context="hacker.l.coldstore.fragments.OutwardFragment">

    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin5"
                android:layout_marginTop="@dimen/margin10"
                android:text="Whole Sale Number*"
                android:textColor="@color/purple"
                android:textSize="@dimen/textSize16" />

            <EditText
                android:id="@+id/edt_wholeSaleNo"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_40_dp"
                android:layout_margin="@dimen/margin5"
                android:background="@drawable/done_corner_round"
                android:hint="Enter Whole Sale Number"
                android:inputType="number"
                android:paddingLeft="@dimen/margin5"
                android:textColor="@color/black"
                android:textSize="@dimen/textSize16" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin5"
                android:layout_marginTop="@dimen/margin10"
                android:text="Name*"
                android:textColor="@color/purple"
                android:textSize="@dimen/textSize16" />

            <TextView
                android:id="@+id/tv_name"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_40_dp"
                android:layout_marginLeft="@dimen/margin5"
                android:layout_marginTop="@dimen/margin10"
                android:background="@drawable/done_corner_round"
                android:gravity="center"
                android:text=""
                android:textColor="@color/black"
                android:textSize="@dimen/textSize16" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin5"
                android:layout_marginTop="@dimen/margin10"
                android:text="Father Name*"
                android:textColor="@color/purple"
                android:textSize="@dimen/textSize16" />

            <TextView
                android:id="@+id/tv_Fname"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_40_dp"
                android:layout_marginLeft="@dimen/margin5"
                android:layout_marginTop="@dimen/margin10"
                android:background="@drawable/done_corner_round"
                android:gravity="center"
                android:text=""
                android:textColor="@color/black"
                android:textSize="@dimen/textSize16" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin5"
                android:layout_marginTop="@dimen/margin10"
                android:text="Mobile Number*"
                android:textColor="@color/purple"
                android:textSize="@dimen/textSize16" />

            <TextView
                android:id="@+id/tv_phone"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_40_dp"
                android:layout_marginLeft="@dimen/margin5"
                android:layout_marginTop="@dimen/margin10"
                android:background="@drawable/done_corner_round"
                android:gravity="center"
                android:text=""
                android:textColor="@color/black"
                android:textSize="@dimen/textSize16" />


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin5"
                android:layout_marginTop="@dimen/margin10"
                android:text="Variety*"
                android:textColor="@color/purple"
                android:textSize="@dimen/textSize16" />

            <TextView
                android:id="@+id/tv_variety"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_40_dp"
                android:layout_marginLeft="@dimen/margin5"
                android:layout_marginTop="@dimen/margin10"
                android:background="@drawable/done_corner_round"
                android:gravity="center"
                android:text=""
                android:textColor="@color/black"
                android:textSize="@dimen/textSize16" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin5"
                android:layout_marginTop="@dimen/margin10"
                android:text="Floor No*"
                android:textColor="@color/purple"
                android:textSize="@dimen/textSize16" />

            <TextView
                android:id="@+id/tv_floor"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_40_dp"
                android:layout_marginLeft="@dimen/margin5"
                android:layout_marginTop="@dimen/margin10"
                android:background="@drawable/done_corner_round"
                android:gravity="center"
                android:text=""
                android:textColor="@color/black"
                android:textSize="@dimen/textSize16" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin5"
                android:layout_marginTop="@dimen/margin10"
                android:text="Rack No*"
                android:textColor="@color/purple"
                android:textSize="@dimen/textSize16" />

            <TextView
                android:id="@+id/tv_rack"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_40_dp"
                android:layout_marginLeft="@dimen/margin5"
                android:layout_marginTop="@dimen/margin10"
                android:background="@drawable/done_corner_round"
                android:gravity="center"
                android:text=""
                android:textColor="@color/black"
                android:textSize="@dimen/textSize16" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin5"
                android:layout_marginTop="@dimen/margin10"
                android:text="Quantity*"
                android:textColor="@color/purple"
                android:textSize="@dimen/textSize16" />

            <TextView
                android:id="@+id/tv_qty"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_40_dp"
                android:layout_marginLeft="@dimen/margin5"
                android:layout_marginTop="@dimen/margin10"
                android:background="@drawable/done_corner_round"
                android:gravity="center"
                android:text=""
                android:textColor="@color/black"
                android:textSize="@dimen/textSize16" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin5"
                android:layout_marginTop="@dimen/margin10"
                android:text="Rent*"
                android:textColor="@color/purple"
                android:textSize="@dimen/textSize16" />

            <TextView
                android:id="@+id/tv_rent"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_40_dp"
                android:layout_marginLeft="@dimen/margin5"
                android:layout_marginTop="@dimen/margin10"
                android:background="@drawable/done_corner_round"
                android:gravity="center"
                android:text=""
                android:textColor="@color/black"
                android:textSize="@dimen/textSize16" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin5"
                android:layout_marginTop="@dimen/margin10"
                android:text="Departure*"
                android:textColor="@color/purple"
                android:textSize="@dimen/textSize16" />

            <EditText
                android:id="@+id/edt_departure"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_40_dp"
                android:layout_margin="@dimen/margin5"
                android:background="@drawable/done_corner_round"
                android:hint="Enter  Number"
                android:inputType="number"
                android:paddingLeft="@dimen/margin5"
                android:textColor="@color/black"
                android:textSize="@dimen/textSize16" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin5"
                android:layout_marginTop="@dimen/margin10"
                android:text="Remaining Quantity*"
                android:textColor="@color/purple"
                android:textSize="@dimen/textSize16" />

            <TextView
                android:id="@+id/tv_Rqty"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_40_dp"
                android:layout_marginLeft="@dimen/margin5"
                android:layout_marginTop="@dimen/margin10"
                android:background="@drawable/done_corner_round"
                android:gravity="center"
                android:text=""
                android:textColor="@color/black"
                android:textSize="@dimen/textSize16" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin5"
                android:layout_marginTop="@dimen/margin10"
                android:text="Address"
                android:textColor="@color/purple"
                android:textSize="@dimen/textSize16" />

            <TextView
                android:id="@+id/tv_address"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_40_dp"
                android:layout_marginLeft="@dimen/margin5"
                android:layout_marginTop="@dimen/margin10"
                android:background="@drawable/done_corner_round"
                android:gravity="center"
                android:text=""
                android:textColor="@color/black"
                android:textSize="@dimen/textSize16" />

            <Button
                android:id="@+id/btn_proced"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_margin="@dimen/margin10"
                android:background="@drawable/capsule_button"
                android:text="Submit Outward"
                android:textAllCaps="false"
                android:textColor="@color/white"
                android:textSize="@dimen/textSize16" />
        </LinearLayout>
    </android.support.v4.widget.NestedScrollView>
</FrameLayout>
